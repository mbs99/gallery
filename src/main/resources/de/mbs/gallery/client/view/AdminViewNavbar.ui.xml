<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:HTMLPanel>
        <div class="row">
            <div class="twelve columns">
                <img src="api/logo" class="logo"/>
            </div>
        </div>
        <div class="row">
            <div class="twelve columns">
                <button class="button u-pull-right" id="logoutButton">Abmelden</button>
                <button class="button u-pull-right" id="orderAdminButton">Bestellungen</button>
                <button class="button u-pull-right" id="galleryAdminButton">Galerie</button>
                <button class="button u-pull-right" id="adminButton">Benutzer</button>
            </div>
        </div>
    </g:HTMLPanel>

</ui:UiBinder>