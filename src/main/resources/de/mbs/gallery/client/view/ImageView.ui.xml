<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:view="urn:import:de.mbs.gallery.client.view">

	<g:HTMLPanel ui:field="imageViewPanel">
		<g:HTMLPanel styleName="row">
			<g:HTMLPanel styleName="twelve columns" ui:field="imageContainer">
			</g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="row">
			<g:HTMLPanel styleName="twelve columns" ui:field="voterContainer">
			</g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="row" ui:field="buttonPanel">
			<div class="four columns">
				<button class="button" id="previousImageButton">Zurück</button>
			</div>
			<div class="four columns">
				<button class="button" id="nextImageButton">Vor</button>
			</div>
			<div class="four columns">
				<button class="button" id="galleryButton">Zur Übersicht</button>
			</div>
		</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder>